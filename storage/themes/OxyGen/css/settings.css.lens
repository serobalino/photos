	width: 43px;
	height: 43px;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -12px;
	z-index: 999;
	opacity: 0;
	-webkit-transition: opacity 0.5s;
	transition: opacity 0.5s
}
#ss_spinner.loading {
	opacity: 1
}
